{
  "name": "@paperdave/logger",
  "version": "2.3.0",
  "description": "Versitle logging with spinners and progress bars.",
  "keywords": [
    "logger",
    "bun"
  ],
  "homepage": "https://github.com/paperdave/various/tree/master/packages/paperdave-logger",
  "repository": "github:paperdave/various",
  "license": "MIT",
  "author": "Dave Caruso <dave@davecode.net>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "screenshot.png"
  ],
  "scripts": {
    "build": "tsc && esbuild --outfile=./dist/index.js --target=esnext --format=esm --platform=node --sourcemap --bundle src/index.ts --external:wrap-ansi --external:chalk --external:@paperdave/utils --external:ansi-escapes && esbuild --outfile=./dist/index.cjs --target=esnext --format=cjs --platform=node --sourcemap --bundle src/index.ts --external:wrap-ansi --external:@paperdave/utils"
  },
  "dependencies": {
    "@paperdave/utils": "workspace:^1.5.0",
    "ansi-escapes": "^6.0.0",
    "chalk": "^5.1.2",
    "strip-ansi": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/wrap-ansi": "^8.0.1",
    "bun-types": "^0.1.2",
    "esbuild": "^0.14.39",
    "mitata": "^0.1.6",
    "typescript": "^4.6.4"
  },
  "engines": {
    "bun": ">=0.1.9",
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "paperdave-status": "stable"
}
